# SambaNova Cloud
SAMBANOVA_API_KEY=

# Google AI Studio (Gemini)
GOOGLE_AI_API_KEY=

# Mistral AI
MISTRAL_API_KEY=

# Cohere
COHERE_API_KEY=

# Hugging Face
HUGGINGFACE_API_KEY=

# OpenRouter (Fallback gateway)
OPENROUTER_API_KEY=
